{% if nbPressions > 0 %} 
    <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="heading{{pressionGroupe.cdGroupe }}">
              <h4 class="panel-title">
                <a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse{{ pressionGroupe.cdGroupe }}" aria-expanded="true" aria-controls="collapse{{ pressionGroupe.cdGroupe }}">
                  {{pressionGroupe.libelle}}
                </a>
              </h4>
            </div>
            <div id="collapse{{ pressionGroupe.cdGroupe }}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading{{ pressionGroupe.cdGroupe }}">
              <div class="panel-body">
                   <table class="table table-bordered table-condensed table-advance" id="idTable{{ pressionGroupe.cdGroupe }}">
                    <thead>
                        <tr>
                            <th  width="5%" nowrap>Action</th>
                            <th  width="70%">libelle</th>
                            <th  width="10%" nowrap>Valeur provisoire</th>
                            <th  width="10%" nowrap>Proposition STL</th>
                          </tr>
                    </thead>
                    <tbody>
                            {% for etat in etats %}
                                <div id="divEtat{{ pressionGroupe.cdGroupe }}_{{ pression.etatType.cdEtat }}" >
                                           {{ render(controller('AeagEdlBundle:Pression:pressionListProposed', {'euCd':pression.masseEau.euCd, 'cdPression':pression.etatType.cdEtat})) }}
                               </div>     
                           {% endfor %}
                          </tbody>
                </table>
              </div>
            </div>
          </div>
    
{% endif %} 


