{% if nbImpacts > 0 %} 

    <dt>{{impactGroupe.libelle}}</dt>
    <dd>
    <table class="tablehead" cellspacing="0" width="100%" align="center">
        <thead>
            <tr>
                <th  width="70%">Libelle</th>
                <th  width="10%" nowrap>Valeur provioire</th>
                <th  width="10%" nowrap>Proposition STL</th>
                <th  width="10%" nowrap>Action</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td colspan=4>

                        {% for impact in impacts %}

                             <div id="divImpact{{ impact.cdImpact }}" >
                                    {% render "AeagEdlBundle:Impact:impactListProposed" with {'euCd':impact.euCd, 'cdImpact':impact.cdImpact}, {'ignore_errors':false} %}
                              </div>     
                       
                        {% endfor %}

                    </td>
                </tr>
            </tbody>
        </table>
    </dd>
{% endif %} 