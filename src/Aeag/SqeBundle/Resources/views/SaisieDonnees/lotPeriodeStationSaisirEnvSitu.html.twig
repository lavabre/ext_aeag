                                                                                                                          {% else %}
                                                                                                                               <tr class="danger" data-toggle="tooltip" data-placement="bottom" title="{{ paramAn.pgCmdMesureEnv.libelleStatut}}">
                                                                                                                          {% endif %}
                                                                                                                    {% else %}
                                                                                                                       <tr>
                                                                                                                   {% endif %}
