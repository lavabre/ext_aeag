                                                                                                                                           <tr>
                                                                                                                                          {% endif %}
                                                                                                                                      {% else %}
                                                                                                                                         <tr>
                                                                                                                                     {% endif %}
                                                                                                                                      
                                                                                                                                     {% if typeMilieu.codeMilieu == 'LPC' %}
                                                                                                                                            <td align="right">
                                                                                                                                            {% if pgCmdPrelevPc %}
                                                                                                                                              {{ pgCmdPrelevPc.profondeur | number_format( 2,","," ")  }}
                                                                                                                                             {% endif %}
                                                                                                                                            </td>
                                                                                                                                       {% endif %}
                                                                                                                                       
                                                                                                                                       <td nowrap>{{ paramAn.paramAn.codeParametre.codeParametre }}</td>
                                                                                                                                       
